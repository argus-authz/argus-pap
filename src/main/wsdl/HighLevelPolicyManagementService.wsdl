			<wsdl:output message="tns:fqanJobPriorityResponse"></wsdl:output>
		</wsdl:operation>
	</wsdl:portType>

	<wsdl:binding name="HighLevelPolicyManagementSOAP" type="tns:HighLevelPolicyManagement">
