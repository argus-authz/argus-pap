				</xsd:sequence>
			</xsd:complexType>
			<xsd:complexType name="UnbanResult">
				<xsd:sequence>
					<xsd:element name="statusCode" type="xsd:int"></xsd:element>
					<xsd:element name="conflictingPolicies" type="tns:StringList">
