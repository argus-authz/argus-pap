	</wsdl:message>
	<wsdl:message name="fqanJobPriorityResponse">
		<wsdl:part name="policyId" type="xsd:string"></wsdl:part>
	</wsdl:message>
	
	<wsdl:portType name="HighLevelPolicyManagement">
		<wsdl:operation name="banDN">
			<wsdl:input message="tns:banDNRequest" />
			<wsdl:output message="tns:banDNResponse" />
		</wsdl:operation>
		<wsdl:operation name="banFQAN">
			<wsdl:input message="tns:banFQANRequest"></wsdl:input>
			<wsdl:output message="tns:banFQANResponse"></wsdl:output>
		</wsdl:operation>
		<wsdl:operation name="unbanDN">
			<wsdl:input message="tns:unbanDNRequest"></wsdl:input>
