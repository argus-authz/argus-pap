        <wsdl:part name="unbanResult" type="tns:UnbanResult"></wsdl:part>
	</wsdl:message>

	<wsdl:message name="unbanFQANRequest">
		<wsdl:part name="fqan" type="xsd:string"></wsdl:part>
	</wsdl:message>
	<wsdl:message name="unbanFQANResponse">
		<wsdl:part name="unbanresult" type="tns:UnbanResult"></wsdl:part>
	</wsdl:message>

	<wsdl:message name="dnJobPriorityRequest">
