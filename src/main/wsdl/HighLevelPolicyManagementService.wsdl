		</wsdl:operation>

	</wsdl:binding>

	<wsdl:service name="HighLevelPolicyManagementService">
		<wsdl:port binding="tns:HighLevelPolicyManagementSOAP" name="HighLevelPolicyManagementService">
			<soap:address location="https://localhost:8443/pap" />
